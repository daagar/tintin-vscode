{
    "name": "TinTinPlusPlus",
    "displayName": "TinTin++",
    "description": "Syntax highlighting for TinTin++",
    "version": "0.0.1",
    "publisher": "daagar",
    "repository": "https://github.com/daagar/tintin-vscode.git",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tt++",
            "aliases": ["TinTin++", "tt++"],
            "extensions": [".tt",".tin"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tt++",
            "scopeName": "source.tin",
            "path": "./syntaxes/tin.tmLanguage"
        }]
    }
}